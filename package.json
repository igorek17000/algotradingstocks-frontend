  "scripts": {
    "start": "env-cmd -f .env.development craco start",
    "start:prod": "env-cmd -f .env.production craco start",
    "start:dev": "env-cmd -f .env.development craco start",
    "build:dev": "env-cmd -f .env.development craco build",
    "build:prod": "env-cmd -f .env.production craco build",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint . --fix --ext .js,.ts,.tsx",
    "format": "./node_modules/.bin/prettier --config .prettierrc 'src/**/*' --write --ignore-unknown",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "./node_modules/.bin/eslint . --fix --ext .js,.ts,.tsx",
      "./node_modules/.bin/prettier --config .prettierrc 'src/**/*' --write --ignore-unknown"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "@types/d3-format": "^3.0.1",
    "@types/d3-time-format": "^4.0.0",
    "@types/react-date-range": "^1.4.3",
    "@types/react-datepicker": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^9",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.3",
    "postcss": "^7",
    "prettier": "^2.4.1",
    "sass": "^1.45.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  }
}
